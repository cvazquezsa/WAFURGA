	<SBX-Section>
		<SBX-Report-Id/>
		<SBX-Datasets>
			<SBX-Dataset name="RegisterActivity">
				<SBX-SQL><![CDATA[SELECT 
  RA.Amount,
  RA.CreationDate,
  DT.Name       AS [DocumentType.Name],
  S.Number      AS [Store.Number],
  S.Name        AS [Store.Name],
  R.Workstation AS [Register.Workstation],
  P.Name        AS [Cashier.Name],
  RA.Comment
FROM RegisterActivity RA
INNER JOIN Register R ON R.UUID = RA.RegisterUUID
INNER JOIN Store S ON S.ID = R.StoreID
LEFT JOIN [User] U ON U.ID = RA.CreationUserID
LEFT JOIN Person P on P.ID = U.PersonID
LEFT JOIN DocumentType DT ON DT.ID=RA.DocumentTypeID
WHERE RA.UUID = '@RegisterActivity.UUID']]></SBX-SQL>
				<SBX-Definition>
					<DATAPACKET Version="2.0">
						<METADATA>
