Formula=(CompraD.Cantidad-CompraD.CantidadCancelada)*CompraD.CantidadInventario/CompraD.Cantidad

[Pendiente]
Clave=Pendiente
TipoDatos=NumFlotante
TipoCampo=Calculado
Monetario=
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Pendiente
CondicionDespliege=(MovTipo(<T>COMS<T>, Compra:Compra.Mov)=COMS.PR) y (Compra:Compra.Estatus en (EstatusConcluido, EstatusPendiente))
Formula=Si((MovTipo(<T>COMS<T>, Compra:Compra.Mov)=COMS.PR) y (Compra:Compra.Estatus en (EstatusConcluido, EstatusPendiente)),<BR>SQL(<T>spVerPresupuestoCompraPendiente :nid, :tEmpresa, :fFecha, :fReq, :tArt, :tSub, :tMoneda, :tCC, :tCat, @ContUso2=:tCC2, @ContUso3=:tCC3<T>,Compra:Compra.ID, Empresa, Compra:Compra.FechaEmision, CompraD.FechaRequerida, CompraD.Articulo, CompraD.SubCuenta, Compra:Compra.Moneda, CompraD.ContUso, CompraD.Categoria, CompraD.ContUso2, CompraD.ContUso3))

