Formula=Si(Config.CompraVerClaveProveedor, ArtClaveProveedor(CompraD.Articulo, CompraD.SubCuenta, Compra:Compra.Proveedor))

[SubImpuesto2]
Clave=SubImpuesto2
TipoDatos=NumMonetario
TipoCampo=Calculado
Monetario=S
TamanoValidacion=10
Alineacion=por Omisión
Nombre=IEPS
CondicionDespliege=(Config.CompraVerImpuesto2) y (Version.Impuesto2Info = 0)
Formula=Si(Version.Impuesto2Info, 0, ImportePorcentaje(SubTotal, CompraD.Impuesto2))

[SubImpuesto1]
Clave=SubImpuesto1
TipoDatos=NumMonetario
TipoCampo=Calculado
Monetario=S
TamanoValidacion=10
Alineacion=por Omisión
Nombre=IVA
CondicionDespliege=Config.CompraVerImpuesto1
