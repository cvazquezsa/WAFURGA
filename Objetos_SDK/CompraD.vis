TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Impuestos
Monetario=S
Formula=SubImpuesto1+SubImpuesto2+SubImpuesto3+SubImpuesto5

[CantidadNeta]
Clave=CantidadNeta
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Cantidad
CondicionDespliege=(Compra:Compra.Estatus noen (EstatusSinAfectar, EstatusPorConfirmar)) y (Usuario.ConsultarCompraPendiente o (MovTipo(<T>COMS<T>, Compra:Compra.Mov) noen (COMS.O, COMS.OP, COMS.OG, COMS.OD, COMS.OI)))
Cantidad=S
Formula=CompraD.Cantidad-CompraD.CantidadCancelada

[ClaveProveedor]
